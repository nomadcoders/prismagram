{
  "name": "prismagram",
  "version": "1.0.0",
  "description": "Instragram clone with Express + Prisma + React and React Native",
  "repository": "https://github.com/nomadcoders/prismagram",
  "author": "Nicolás Serrano Arévalo <itnico.las.me@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.4.5",
    "aws-sdk": "^2.483.0",
    "copy": "^0.3.2",
    "dotenv": "^6.2.0",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.4.0",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.16",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prisma": "^1.33.0",
    "prisma-client-lib": "^1.26.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.4.4",
    "nodemon": "^1.18.10"
  },
  "scripts": {
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "yarn run deploy && yarn run generate",
    "dev": "nodemon --exec babel-node src/server.js",
    "prebuild": "yarn run generate",
    "build": "babel src -d build",
    "postbuild": "cd src && copy ./api/**/*.graphql ../build/api/",
    "start": "node build/server.js"
  }
}
